{
  "name": "crazystack",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "and": "cd android && gradlew.bat clean && cd .. && react-native run-android",
    "ios": "expo run:ios",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "c":"react-native clean-project-auto",
    "adb": "cd android/app/build/outputs/apk/release && adb install -r app-release.apk",
    "apk": "rm -rf android/app/build && yarn install && cd android && ./gradlew clean && ./gradlew assembleRelease",
    "reverse": " adb reverse tcp:9090 tcp:9090",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@types/react": "~18.2.14",
    "expo": "~49.0.15",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6",
    "typescript": "^5.1.3",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.1",
    "@types/jest": "^29.5.11",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "plop": "^4.0.0",
    "prettier": "^3.1.0",
    "react-native-clean-project": "^4.0.3",
    "react-native-svg-transformer": "^1.1.0",
    "reactotron-react-native": "^5.0.3"
  },
  "private": true
